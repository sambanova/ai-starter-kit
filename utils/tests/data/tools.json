        "type": "function",
        "function": {
            "name": "exa_news_search",
            "description": "partial(func, *args, **keywords) - new function with partial application of the given arguments and keywords.",
            "parameters": {
                "type": "object",
                "properties": {
                    "query": {
                        "description": "query",
                        "title": "Query",
                        "type": "string"
                    },
                    "answer": {
                        "default": false,
                        "description": "answer",
                        "title": "Answer",
                        "type": "boolean"
                    }
                },
                "required": ["query"]
            }
        }
    },
    "sum_of_integers": {
