  "convert_units": {
    "type": "function",
    "function": {
      "name": "convert_units",
      "description": "Convert between common measurement units (supports bidirectional conversions)",
      "parameters": {
        "type": "object",
        "properties": {
          "value": {
            "type": "number"
          },
          "from_unit": {
            "type": "string",
            "enum": [
              "miles", "km", "inches", "cm", "feet", "meters",
              "pounds", "kg", "ounces", "grams", "gallons", "liters",
              "fahrenheit", "celsius"
            ]
          },
          "to_unit": {
            "type": "string",
            "enum": [
              "miles", "km", "inches", "cm", "feet", "meters",
              "pounds", "kg", "ounces", "grams", "gallons", "liters",
              "fahrenheit", "celsius"
            ]
          }
        },
        "required": ["value", "from_unit", "to_unit"]
      }
    }
  },
  "convert_currency": {
    "type": "function",
    "function": {
      "name": "convert_currency",
      "description": "Convert between currencies using exchange rate",
      "parameters": {
        "type": "object",
        "properties": {
          "amount": {
            "type": "number",
            "description": "Amount to convert"
          },
          "from_currency": {
            "type": "string",
            "enum": ["USD", "EUR", "GBP"]
          },
          "to_currency": {
            "type": "string",
            "enum": ["USD", "EUR", "GBP"]
          },
          "exchange_rate": {
            "type": "number",
            "description": "Optional custom exchange rate"
          }
        },
        "required": ["amount", "from_currency", "to_currency"]
      }
    }
  },
